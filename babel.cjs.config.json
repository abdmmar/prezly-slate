{
    "extends": "./babel.config.json",
    "presets": [
        ["@babel/env", {
            "useBuiltIns": false,
            "modules": "commonjs",
            "targets": "> 0.25%, not dead"
        }]
    ],
    "plugins": [
        [
            "babel-plugin-module-resolver",
            { "alias": { "#lodash": "lodash" } },
            "lodash-conditional-dependency"
        ],
        ["babel-plugin-add-import-extension", { "extension": "cjs" }]
    ]
}
