{
    "name": "@prezly/slate-lists",
    "version": "0.10.1",
    "description": "The best Slate lists extension out there",
    "license": "MIT",
    "type": "module",
    "main": "build/esm/index.mjs",
    "module": "build/esm/index.mjs",
    "types": "build/types/index.d.ts",
    "exports": {
        ".": {
            "types": "./build/types/index.d.ts",
            "import": "./build/esm/index.mjs",
            "default": "./build/esm/index.mjs"
        }
    },
    "files": [
        "build",
        "src"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/prezly/slate.git"
    },
    "bugs": {
        "url": "https://github.com/prezly/slate/issues"
    },
    "homepage": "https://github.com/prezly/slate#readme",
    "scripts": {
        "lint": "eslint ./src --ext=.ts,.tsx",
        "build": "NODE_ENV=production npm run build:all",
        "build:dev": "NODE_ENV=development npm run build:all",
        "build:all": "npm run build:types && npm run build:esm && npm run build:cjs",
        "build:types": "tsc-fancy --preserveConsoleOutput --build .",
        "build:esm": "gulp build:esm",
        "build:cjs": "gulp build:cjs",
        "watch": "concurrently --kill-others --names 'd.ts,esm , cjs ' 'npm run watch:types' 'npm run watch:esm' 'npm run watch:cjs'",
        "watch:types": "npm run build:types -- --watch --pretty",
        "watch:esm": "npm run watch:esm",
        "watch:cjs": "npm run watch:cjs",
        "clean": "npm run clean:build && npm run clean:node_modules",
        "clean:node_modules": "rimraf node_modules/",
        "clean:build": "rimraf build/ *.tsbuildinfo"
    },
    "peerDependencies": {
        "react": "^16.9.0",
        "slate": "^0.71.0",
        "slate-react": "^0.71.0"
    },
    "dependencies": {
        "@prezly/slate-commons": "^0.10.1",
        "@prezly/slate-types": "^0.10.1",
        "uuid": "^8.3.0"
    },
    "devDependencies": {
        "@prezly/slate-hyperscript": "^0.10.1",
        "@types/uuid": "^8.3.0"
    }
}
